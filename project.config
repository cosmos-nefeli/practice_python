[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/snigdha15
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/snigdha15
	label-Verified = -1..+1 group user/snigdha15
	push = group user/snigdha15
	read = group user/snigdha15
	rebase = group user/snigdha15
	removeReviewer = group user/snigdha15
	submit = group user/snigdha15
[access "refs/heads/*"]
	create = group user/snigdha15
	push = +force group user/snigdha15
	pushMerge = group user/snigdha15
	read = group user/snigdha15
[access "refs/tags/*"]
	push = group user/snigdha15
	push = +force group user/snigdha15
